# YieldSmart AI CarBot - Configuration
# Copy this to .env and fill in your values

# API Keys
GOOGLE_GEMINI_API_KEY=your_gemini_api_key_here
GROQ_API_KEY=your_groq_api_key_here
GOOGLE_SHEETS_API_KEY=your_sheets_api_key

# Google Sheets
GOOGLE_SHEET_ID=your_google_sheet_id
WEBHOOK_URL=https://webhook.cool/your-webhook-id

# OLX Accounts (8 Cities)
OLX_DELHI_EMAIL=delhi@example.com
OLX_DELHI_PASS=password123

OLX_MUMBAI_EMAIL=mumbai@example.com
OLX_MUMBAI_PASS=password123

OLX_PUNE_EMAIL=pune@example.com
OLX_PUNE_PASS=password123

OLX_SURAT_EMAIL=surat@example.com
OLX_SURAT_PASS=password123

OLX_PATNA_EMAIL=patna@example.com
OLX_PATNA_PASS=password123

OLX_INDORE_EMAIL=indore@example.com
OLX_INDORE_PASS=password123

OLX_PUNJAB_EMAIL=punjab@example.com
OLX_PUNJAB_PASS=password123

OLX_LUCKNOW_EMAIL=lucknow@example.com
OLX_LUCKNOW_PASS=password123

# Facebook Accounts
FB_DELHI_EMAIL=delhi_fb@example.com
FB_DELHI_PASS=password123

FB_MUMBAI_EMAIL=mumbai_fb@example.com
FB_MUMBAI_PASS=password123

# Proxy Configuration (Free tier)
PROXY_TYPE=residential
PROXY_ENABLED=true
IP_ROTATION_ENABLED=true

# Bot Settings
RANDOM_DELAY_MIN=500
RANDOM_DELAY_MAX=5000
USER_AGENT_ROTATION=true
COOKIE_ROTATION=true

# WhatsApp Settings
WHATSAPP_ENABLED=true
WHATSAPP_BROWSER=chromium

# Logging
LOG_LEVEL=info
DATABASE_PATH=./data/yieldmart.db
